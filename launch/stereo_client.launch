<launch>

   <!-- arduino Node-->
 <node pkg="rosserial_python" 
	type="serial_node.py" name="serial_node">
  <param name="port" value="/dev/ttyACM0"/>
  <param name="buads" value="115200"/>
  </node> 

  <!-- odompublisher Node-->

  <node pkg="smartcar" 
       type="odompublisher" name="odom_publisher">
  </node>


<include file="$(find joy_control)/launch/joy.launch" />
 
<arg name="camera" default="camera" />
  <!-- start sensor -->
  <node pkg="mycamera" type="camera_splite" name="camera_splite" ns="camera">
        <param name="jpeg_quality" value="70"/>
        <remap from="left/image_raw" to="left/image"/>
        <remap from="right/image_raw" to="right/image"/>
     </node>
 <!-- brocast static tf -->
   <node pkg="tf" type="static_transform_publisher" name="base_link_to_camera_link" args="0.05 0 0.3 0 0 0 base_link camera_link 100"/>
   <node pkg="tf" type="static_transform_publisher" name="camera_link_to_laser" args="0 0 0 0 0 0 camera_link laser 100"/>

</launch>
